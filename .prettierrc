{
  "jsxBracketSameLine": false,
  "noSemi": true,
  "rcVerbose": true,
  "printWidth": 300,
  "singleQuote": false,
  "tabWidth": 2,
  "proseWrap": "always",
  "trailingComma": "none",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "markdown"
      }
    },
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    }
  ]
}
